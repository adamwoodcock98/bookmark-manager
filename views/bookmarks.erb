<form action='/bookmarks/add'>
<button>Add Bookmark</button> 
</form>
<ul>
  <% @bookmarks.each do |bookmark| %> 
    <li> <a href=<%= bookmark.url%>> <%= bookmark.title %> </a> 
      <form action='/bookmarks/view'>
        <input type='submit' value='View' name=<%= bookmark.id %> >
        <input type='hidden' name='id_to_delete' value=<%= bookmark.id %>>
      </form>
    </li>
    
  <% end %>
</ul>